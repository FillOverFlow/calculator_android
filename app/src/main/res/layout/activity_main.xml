<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout android:id="@+id/LinearLayout01"
        android:layout_height="wrap_content" android:layout_width="match_parent">
        <EditText android:id="@+id/EditText01" android:layout_height="wrap_content"
            android:layout_width="match_parent" android:editable="false" />
    </LinearLayout>
    <LinearLayout android:id="@+id/LinearLayout02"
        android:layout_height="wrap_content" android:layout_width="match_parent">
        <EditText android:id="@+id/EditText02" android:layout_height="wrap_content"
            android:layout_width="match_parent" android:editable="false" />
    </LinearLayout>
    <LinearLayout android:id="@+id/LinearLayout03"
        android:layout_width="match_parent" android:layout_height="match_parent"
        android:layout_weight="1">

        <Button
            android:id="@+id/ButtonNumber07"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="7" />

        <Button
            android:id="@+id/ButtonNumber08"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="8" />

        <Button
            android:id="@+id/ButtonNumber09"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="9" />

        <Button
            android:id="@+id/ButtonSymbolDivide"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="/" />
    </LinearLayout>
    <LinearLayout android:id="@+id/LinearLayout04"
        android:layout_width="match_parent" android:layout_height="match_parent"
        android:layout_weight="1">

        <Button
            android:id="@+id/ButtonNumber04"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="4" />

        <Button
            android:id="@+id/ButtonNumber05"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="5" />

        <Button
            android:id="@+id/ButtonNumber06"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="6" />

        <Button
            android:id="@+id/ButtonSymbolMultiply"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="*" />
    </LinearLayout>
    <LinearLayout android:id="@+id/LinearLayout05"
        android:layout_width="match_parent" android:layout_height="match_parent"
        android:layout_weight="1">

        <Button
            android:id="@+id/ButtonNumber01"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="1" />

        <Button
            android:id="@+id/ButtonNumber02"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="2" />

        <Button
            android:id="@+id/ButtonNumber03"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="3" />

        <Button
            android:id="@+id/ButtonSymbolSubtract"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="-" />
    </LinearLayout>
    <LinearLayout android:id="@+id/LinearLayout06"
        android:layout_width="match_parent" android:layout_height="match_parent"
        android:layout_weight="1">

        <Button
            android:id="@+id/ButtonNumber00"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="0" />

        <Button
            android:id="@+id/ButtonSymbolBracketLeft"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="(" />

        <Button
            android:id="@+id/ButtonSymbolBracketRight"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text=")" />

        <Button
            android:id="@+id/ButtonSymbolAdd"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="+" />
    </LinearLayout>
    <LinearLayout android:id="@+id/LinearLayout07"
        android:layout_width="match_parent" android:layout_height="match_parent"
        android:layout_weight="1">

        <Button
            android:id="@+id/ButtonSymbolDot"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="." />

        <Button
            android:id="@+id/ButtonSymbolDel"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="Del" />

        <Button
            android:id="@+id/ButtonSymbolClear"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="Clear" />

        <Button
            android:id="@+id/ButtonSymbolExe"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="=" />
    </LinearLayout>
</LinearLayout>